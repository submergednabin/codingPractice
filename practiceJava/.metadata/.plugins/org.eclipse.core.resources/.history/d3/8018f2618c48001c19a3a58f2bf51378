package com.practice.interview;

import java.util.Scanner;

public class javaNumber {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int N = sc.nextInt();
		int count = 0;
		for(int i=0; i<N; i++) {
			int value = sc.nextInt();
			int temp = value;
			
			if (value == temp) {
				count++;
				System.out.println(count);
			}else {
				count=1;
			}
			
		}
	}
}
